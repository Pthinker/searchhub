{
  "name": "accumulo",
    "description": "The Apache Accumulo project, tweets, wiki, github, et. al",
  "label": "Apache Accumulo",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "accumulo-website",
      "label": "Apache Accumulo Website",
      "url": "http://accumulo.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "accumulo-user",
      "label": "Accumulo User",
      "mbox": "accumulo-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "accumulo-dev",
      "label": "Accumulo Dev",
      "mbox": "accumulo-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-accumulo",
    "label": "Apache Accumulo JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/ACCUMULO",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
