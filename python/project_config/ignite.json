{
  "name": "ignite",
    "description": "The Apache Ignite project, tweets, wiki, github, et. al",
  "label": "Apache Ignite",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "ignite-website",
      "label": "Apache Ignite Website",
      "url": "http://ignite.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "ignite-user",
      "label": "Ignite User",
      "mbox": "ignite-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "ignite-dev",
      "label": "Ignite Dev",
      "mbox": "ignite-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-ignite",
    "label": "Apache Ignite JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/IGNITE",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
