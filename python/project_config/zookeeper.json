{
  "name": "zookeeper",
  "description": "The Apache Zookeeper project, tweets, wiki, github, et. al",
  "label": "Apache Zookeeper",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "zookeeper-website",
      "label": "Apache Zookeeper Website",
      "url": "http://zookeeper.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "zookeeper-user",
      "label": "Zookeeper User",
      "mbox": "zookeeper-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "zookeeper-dev",
      "label": "Zookeeper Dev",
      "mbox": "zookeeper-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-zookeeper",
    "label": "Apache Zookeeper JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/ZOOKEEPER",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 2
      }

  },
  "githubs": [
    {
      "name": "zookeeper-github",
      "label": "Apache Zookeeper Github",
      "url": "https://github.com/apache/zookeeper",
      "github_user": "lucid-ci",
      "github_pass": "GITHUB_KEY",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 1
      }
    }
  ],
  "wikis": [
    {
      "name": "zookeeper-wiki",
      "label": "Zookeeper Wiki",
      "url": "https://cwiki.apache.org/confluence/display/ZOOKEEPER/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ]
}
