{
  "name": "ambari",
    "description": "The Apache Ambari project, tweets, wiki, github, et. al",
  "label": "Apache Ambari",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "ambari-website",
      "label": "Apache Ambari Website",
      "url": "http://ambari.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "ambari-user",
      "label": "Ambari User",
      "mbox": "ambari-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "ambari-dev",
      "label": "Ambari Dev",
      "mbox": "ambari-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-ambari",
    "label": "Apache Ambari JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/AMBARI",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
