{
  "name": "bigtop",
    "description": "The Apache Bigtop project, tweets, wiki, github, et. al",
  "label": "Apache Bigtop",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "bigtop-website",
      "label": "Apache Bigtop Website",
      "url": "http://bigtop.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "bigtop-user",
      "label": "Bigtop User",
      "mbox": "bigtop-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "bigtop-dev",
      "label": "Bigtop Dev",
      "mbox": "bigtop-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-bigtop",
    "label": "Apache Bigtop JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/BIGTOP",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
