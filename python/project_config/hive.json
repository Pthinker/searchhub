{
  "name": "hive",
  "description": "The Apache Hive project, tweets, wiki, github, et. al",
  "label": "Apache Hive",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "hive-website",
      "label": "Apache Hive Website",
      "url": "http://hive.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "hive-user",
      "label": "Hive User",
      "mbox": "hive-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "hive-dev",
      "label": "Hive Dev",
      "mbox": "hive-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-hive",
    "label": "Apache Hive JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/HIVE",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
