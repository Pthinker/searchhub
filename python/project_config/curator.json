{
  "name": "curator",
    "description": "The Apache Curator project, tweets, wiki, github, et. al",
  "label": "Apache Curator",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "curator-website",
      "label": "Apache Curator Website",
      "url": "http://curator.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "curator-user",
      "label": "Curator User",
      "mbox": "curator-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "curator-dev",
      "label": "Curator Dev",
      "mbox": "curator-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-curator",
    "label": "Apache Curator JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/CURATOR",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
