{
  "name": "kylin",
    "description": "The Apache Kylin project, tweets, wiki, github, et. al",
  "label": "Apache Kylin",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "kylin-website",
      "label": "Apache Kylin Website",
      "url": "http://kylin.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "kylin-user",
      "label": "Kylin User",
      "mbox": "kylin-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "kylin-dev",
      "label": "Kylin Dev",
      "mbox": "kylin-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-kylin",
    "label": "Apache Kylin JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/KYLIN",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
