{
  "name": "thrift",
    "description": "The Apache Thrift project, tweets, wiki, github, et. al",
  "label": "Apache Thrift",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "thrift-website",
      "label": "Apache Thrift Website",
      "url": "http://thrift.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "thrift-user",
      "label": "Thrift User",
      "mbox": "thrift-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "thrift-dev",
      "label": "Thrift Dev",
      "mbox": "thrift-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-thrift",
    "label": "Apache Thrift JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/THRIFT",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
