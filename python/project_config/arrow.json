{
  "name": "arrow",
    "description": "The Apache Arrow project, tweets, wiki, github, et. al",
  "label": "Apache Arrow",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "arrow-website",
      "label": "Apache Arrow Website",
      "url": "http://arrow.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "arrow-dev",
      "label": "Arrow Dev",
      "mbox": "arrow-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-arrow",
    "label": "Apache Arrow JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/ARROW",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
