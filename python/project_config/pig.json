{
  "name": "pig",
    "description": "The Apache Pig project, tweets, wiki, github, et. al",
  "label": "Apache Pig",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "pig-website",
      "label": "Apache Pig Website",
      "url": "http://pig.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "pig-user",
      "label": "Pig User",
      "mbox": "pig-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "pig-dev",
      "label": "Pig Dev",
      "mbox": "pig-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-pig",
    "label": "Apache Pig JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/PIG",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
