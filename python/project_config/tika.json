{
  "name": "tika",
  "description": "The Apache Tika project, tweets, wiki, github, et. al",
  "label": "Apache Tika",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "tika-website",
      "label": "Apache Tika Website",
      "url": "http://tika.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "tika-user",
      "label": "Tika User",
      "mbox": "tika-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "tika-dev",
      "label": "Tika Dev",
      "mbox": "tika-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-tika",
    "label": "Apache Tika JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/TIKA",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 1
      }
  },
  "githubs": [
    {
      "name": "tika-github",
      "label": "Apache Tika Github",
      "url": "https://github.com/apache/tika",
      "github_user": "lucid-ci",
      "github_pass": "GITHUB_KEY",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 2
      }
    }
  ],
  "wikis": [
    {
      "name": "tika-wiki",
      "label": "Tika Wiki",
      "url": "http://wiki.apache.org/tika/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ]
}
