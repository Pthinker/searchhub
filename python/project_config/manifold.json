{
  "name": "manifoldcf",
    "description": "The Apache ManifoldCF project, tweets, wiki, github, et. al",
  "label": "Apache ManifoldCF",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "manifoldcf-website",
      "label": "Apache ManifoldCF Website",
      "url": "http://manifoldcf.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "manifoldcf-user",
      "label": "ManifoldCF User",
      "mbox": "manifoldcf-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "manifoldcf-dev",
      "label": "ManifoldCF Dev",
      "mbox": "manifoldcf-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-manifoldcf",
    "label": "Apache ManifoldCF JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/MANIFOLDCF",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
