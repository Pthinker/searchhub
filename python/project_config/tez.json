{
  "name": "tez",
    "description": "The Apache Tez project, tweets, wiki, github, et. al",
  "label": "Apache Tez",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "tez-website",
      "label": "Apache Tez Website",
      "url": "http://tez.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "tez-user",
      "label": "Tez User",
      "mbox": "tez-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "tez-dev",
      "label": "Tez Dev",
      "mbox": "tez-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-tez",
    "label": "Apache Tez JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/TEZ",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
