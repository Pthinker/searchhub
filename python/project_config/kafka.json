{
  "name": "kafka",
    "description": "The Apache Kafka project, tweets, wiki, github, et. al",
  "label": "Apache Kafka",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "kafka-website",
      "label": "Apache Kafka Website",
      "url": "http://kafka.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "kafka-user",
      "label": "Kafka User",
      "mbox": "kafka-users",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "kafka-dev",
      "label": "Kafka Dev",
      "mbox": "kafka-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-kafka",
    "label": "Apache Kafka JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/KAFKA",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
