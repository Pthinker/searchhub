{
  "name": "spark",
  "description": "The Apache Spark project, tweets, wiki, github, et. al",
  "label": "Apache Spark",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "spark-website",
      "label": "Apache Spark Website",
      "url": "http://spark.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "spark-user",
      "label": "Spark User",
      "mbox": "spark-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "spark-dev",
      "label": "Spark Dev",
      "mbox": "spark-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-spark",
    "label": "Apache Spark JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/SPARK",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 3
      }
  },
  "githubs": [
    {
      "name": "spark-github",
      "label": "Apache Spark Github",
      "url": "https://github.com/apache/spark",
      "github_user": "lucid-ci",
      "github_pass": "GITHUB_KEY",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ]
}
