{
  "name": "jclouds",
    "description": "The Apache jclouds project, tweets, wiki, github, et. al",
  "label": "Apache jclouds",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "jclouds-website",
      "label": "Apache jclouds Website",
      "url": "https://jclouds.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "jclouds-user",
      "label": "jclouds User",
      "mbox": "jclouds-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "jclouds-dev",
      "label": "jclouds Dev",
      "mbox": "jclouds-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-jclouds",
    "label": "Apache jclouds JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/jclouds",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
