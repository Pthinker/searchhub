{
  "name": "oozie",
    "description": "The Apache Oozie project, tweets, wiki, github, et. al",
  "label": "Apache Oozie",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "oozie-website",
      "label": "Apache Oozie Website",
      "url": "http://oozie.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "oozie-user",
      "label": "Oozie User",
      "mbox": "oozie-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "oozie-dev",
      "label": "Oozie Dev",
      "mbox": "oozie-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-oozie",
    "label": "Apache Oozie JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/OOZIE",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 1
      }
  }
}
