{
  "name": "cassandra",
    "description": "The Apache Cassandra project, tweets, wiki, github, et. al",
  "label": "Apache Cassandra",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "cassandra-website",
      "label": "Apache Cassandra Website",
      "url": "http://cassandra.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "cassandra-user",
      "label": "Cassandra User",
      "mbox": "cassandra-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "cassandra-dev",
      "label": "Cassandra Dev",
      "mbox": "cassandra-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 1
      }
    }
  ],
  "jira": {
    "name": "apache-jira-cassandra",
    "label": "Apache Cassandra JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/CASSANDRA",
    "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
