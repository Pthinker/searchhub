{
  "name": "uima",
    "description": "The Apache Uima project, tweets, wiki, github, et. al",
  "label": "Apache Uima",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "uima-website",
      "label": "Apache Uima Website",
      "url": "http://uima.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "uima-user",
      "label": "Uima User",
      "mbox": "uima-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "uima-dev",
      "label": "Uima Dev",
      "mbox": "uima-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-uima",
    "label": "Apache Uima JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/UIMA",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 2
      }
  }
}
