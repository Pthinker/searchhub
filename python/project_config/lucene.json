{
  "name": "lucene",
  "description": "The Apache Lucene project, tweets, wiki, github, et. al",
  "label": "Apache Lucene",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "lucene-website",
      "label": "Apache Lucene Website",
      "url": "http://lucene.apache.org/core",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "lucene-user",
      "label": "Lucene User",
      "mbox": "lucene-java-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "lucene-general",
      "label": "Lucene General",
      "mbox": "lucene-general",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "lucene-dev",
      "label": "Lucene Dev",
      "mbox": "lucene-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-lucene",
    "label": "Apache Lucene JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/LUCENE",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
