{
  "name": "hbase",
  "description": "The Apache HBase project, tweets, wiki, github, et. al",
  "label": "Apache HBase",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "hbase-website",
      "label": "Apache HBase Website",
      "url": "http://hbase.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "hbase-user",
      "label": "HBase User",
      "mbox": "hbase-user",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "hbase-dev",
      "label": "HBase Dev",
      "mbox": "hbase-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-hbase",
    "label": "Apache HBase JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/HBASE",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
